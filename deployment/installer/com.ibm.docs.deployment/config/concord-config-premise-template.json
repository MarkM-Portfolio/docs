{
	"component":
	{
		"components":
		[
			{
				"id":"com.ibm.docs.repository",
				"class":"com.ibm.docs.repository.RepositoryComponent",
				"config":
				{
					"adapters" :
					[
						{
							"id" : "lcfiles",
						  	"class" : "com.ibm.docs.repository.files.LCFilesCMISRepository",
							"config" :
							{
								"server_url" : "$files_url",
								"j2c_alias" : "$ic_admin_j2c_alias"
							}
						},
						{
							"id" : "external.cmis",
							"class" : "com.ibm.docs.repository.external.cmis.ExternalCMISRepository",
							"config" :
							{
								"s2s_method" : "$external_s2s_method",
								"customer_id" : "$external_customer_id",
								"oauth2_endpoint" : "$external_oauth_endpoint",
								"oauth2_authorize_endpoint": "$external_oauth_authorize",
								"j2c_alias" : "$external_j2c_alias",
								"s2s_token" : "$external_s2s_token",
								"token_key" : "$external_token_key",
								"onbehalf_header" : "$external_as_user_key",
								"atom_pub" : "$external_cmis_atom_pub",
								"object_store" : "$external_object_store",
								"docs_callback_endpoint" : "$docs_call_back_url",
								"repository_home" : "$external_repository_home",
								"repository_name" :""
							}
						},
						{
							"id" : "external.rest",
							"class" : "com.ibm.docs.repository.external.rest.ExternalRestRepository",
							"config" :
							{
								"s2s_method" : "$external_s2s_method",
								"customer_id" : "$external_customer_id",
								"oauth2_endpoint" : "$external_oauth_endpoint",
								"oauth2_authorize_endpoint": "$external_oauth_authorize",
								"j2c_alias" : "$external_j2c_alias",
								"s2s_token" : "$external_s2s_token",
								"token_key" : "$external_token_key",
								"onbehalf_header" : "$external_as_user_key",
								"media_meta_url" : "$external_meta_url",
								"media_get_url" : "$external_get_content_url",
								"media_set_url" : "$external_set_content_url",
								"docs_callback_endpoint" : "$docs_call_back_url",
								"repository_home" : "$external_repository_home",
								"repository_name" :""
							}
						}
					],
					"default" : "lcfiles"
				}
			},
			{
				"id":"com.ibm.concord.platform.task",
				"class":"com.ibm.concord.platform.task.TaskComponentImpl",
				"config":
				{
					"adapter":
						{
							"class" : "com.ibm.concord.platform.task.DocsActivityAdapter",
							"config" :
							{
								"message_type" : "TASK"
							}
						}
				}
			},
			{
				"id":"com.ibm.concord.platform.email.notice",
				"class":"com.ibm.concord.platform.notification.EmailNoticeComponentImpl",
				"config":
				{
					"adapters" :
					[
						{
							"id" : "lcfiles",
						  	"class" : "com.ibm.concord.lc3.notification.ConnsEmailNoticeAdapter",
							"config" :
							{
								"server_url" : "$email_url",
								"restful_url" : "/opensocial/rest/activitystreams/",
								"j2c_alias" : "$ic_admin_j2c_alias",
								"generator":
								{
									"id":"IBMDocs",
									"displayName": "Docs",
									"url": "http://www.hcl.com/"
								}
							}
						}
					],
					"default" : "lcfiles"
				}
			},
			{
				"id":"com.ibm.concord.platform.conversion",
				"class":"com.ibm.concord.platform.conversion.ConversionComponentImpl",
				"config":
				{
					"conversionService":
					{
							"serviceurl":"$conversion_url/ConversionService",
							"resulturl":"$conversion_url/ConversionResult",
							"numberofretry":"120",
							"intervalofretry":"1000",
							"socketTimeout":"30000",
							"s2s_token" : "fallseason2011",
							"j2c_alias" : "docsAdmin"
					}
				}
			},
			{
				"id":"com.ibm.concord.platform.draft",
				"class":"com.ibm.concord.platform.draft.DraftComponent",
				"config":
				{
					"adapter" :
					{
						"id" : "nls",
						"class" : "com.ibm.concord.draft.spi.impl.NFSDraftStorageAdapterFactory"
					}
				}
			},
			{
				"id":"com.ibm.concord.platform.revision",
				"class":"com.ibm.concord.revision.service.RevisionComponent",
				"config":
				{
					"adapter" :
					{
						"storage-adapter":
						{
						"id" : "nfs",
						"class" : "com.ibm.concord.revision.storage.NFSRevisionStorageAdapterFactory"
						},
						"meta-adapter":
						{
						"id" : "db",
						"class": "com.ibm.concord.revision.meta.DBRevisionMetadataAdapterFactory"
						}
					}
				}
			},
			{
				"id":"com.ibm.docs.authentication",
				"class":"com.ibm.docs.authentication.AuthenticationComponent",
				"config":
				{
					"adapters" :
					[
						{
							"id" : "lcfiles",
							"class" : "com.ibm.docs.authentication.filters.LotusConnectionsAuth",
							"config" :
							{
								"auth_type": "$auth_type",
								"auth_host": "$auth_host"
							}
						},
						{
							"id" : "external.cmis",
							"class":"com.ibm.docs.authentication.filters.ExternalAuth",
							"config" : {
								"s2s_method" : "$external_s2s_method"
							}
						},
						{
							"id" : "external.rest",
							"class":"com.ibm.docs.authentication.filters.ExternalAuth",
							"config" : {
								"s2s_method" : "$external_s2s_method"
							}
						}
					],
					"default" : "lcfiles"
				}
			},
			{
				"id":"com.ibm.docs.directory",
				"class":"com.ibm.docs.directory.DirectoryComponent",
				"config":
				{
					"adapters":
					[
						{
							"id" : "lcfiles",
							"class":"com.ibm.docs.directory.connections.LotusConnectionsDirectory",
							"config" :
							{
							}
						},
						{
							"id" : "external.cmis",
							"class":"com.ibm.docs.directory.external.ExternalDirectory",
							"config" : {
								"profiles_url" : "$external_profiles_url",
								"current_user_profiles_url" : "$external_current_user_profiles_url",
								"s2s_method" : "$external_s2s_method",
								"customer_id" : "$external_customer_id",
								"oauth2_endpoint" : "$external_oauth_endpoint",
								"oauth2_authorize_endpoint": "$external_oauth_authorize",
								"j2c_alias" : "$external_j2c_alias",
								"s2s_token" : "$external_s2s_token",
								"token_key" : "$external_token_key",
								"onbehalf_header" : "$external_as_user_key",
								"docs_callback_endpoint" : "$docs_call_back_url",
								"keys" : {
									"id_key" : "id",
									"name_key" : "name",
									"display_name_key" : "display_name",
									"email_key" : "email",
									"photo_url_key" : "photo_url",
									"org_id_key" : "org_id",
									"url_query_key" : "userid"
								}
							}
						},
						{
							"id" : "external.rest",
							"class":"com.ibm.docs.directory.external.ExternalDirectory",
							"config" : {
								"profiles_url" : "$external_profiles_url",
								"current_user_profiles_url" : "$external_current_user_profiles_url",
								"s2s_method" : "$external_s2s_method",
								"customer_id" : "$external_customer_id",
								"oauth2_endpoint" : "$external_oauth_endpoint",
								"oauth2_authorize_endpoint": "$external_oauth_authorize",
								"j2c_alias" : "$external_j2c_alias",
								"s2s_token" : "$external_s2s_token",
								"token_key" : "$external_token_key",
								"onbehalf_header" : "$external_as_user_key",
								"docs_callback_endpoint" : "$docs_call_back_url",
								"keys" : {
									"id_key" : "id",
									"name_key" : "name",
									"display_name_key" : "display_name",
									"email_key" : "email",
									"photo_url_key" : "photo_url",
									"org_id_key" : "org_id",
									"org_name_key": "org_name",
									"url_query_key" : "userid",
									"job_title_key": "job_title"
								}
							}
						}
					],
					"default" : "lcfiles"
				}
			},
			{
				"id":"com.ibm.concord.platform.dao",
				"class":"com.ibm.concord.platform.dao.DataAccessComponentImpl",
				"config":
				{
				}
			},
			{
				"id":"com.ibm.concord.document.services",
				"class":"com.ibm.concord.document.services.DocumentServiceComponentImpl",
				"config":
				{
					"providers" : [
						{
							"comments":"The items in this section are set by default and are not allowed to be customized.",
							"name":"text",
							"class":"com.ibm.concord.writer.TextDocumentService",
							"config":
							{
								"CLEnabled": "true",
								"DocTrackChange": "true",
								"limits":
								{
									"max-size": "30720",
									"actual-max-size": "30720",
									"max-image-size": "8192",
									"max-html-size":"4096"
								},
								"mobile_limits":
								{
									"max-size": "30720",
									"actual-max-size": "30720",
									"max-image-size": "8192",
									"max-html-size":"4096"
								}
							}
						},
						{
							"comments":"The items in this section are set by default and are not allowed to be customized.",
							"name":"sheet",
							"class":"com.ibm.concord.spreadsheet.SpreadsheetDocumentService",
							"config":
							{
								"CLEnabled": "true",
								"ACLEnabled":"true",
								"NodeJSEnabled": "$nodejs_enabled",
								"UseReferenceJS": "false",
								"partial-level": "2",
								"partial-row-count": "200",
								"limits":
								{
									"max-size": "20480",
									"actual-max-size": "20480",
									"max-image-size": "8192",
									"max-text-size": "5120",
									"max-sheet-rows": "30000"
								},
								"mobile_limits":
								{
									"max-size": "2048",
									"actual-max-size": "2048",
									"max-image-size": "8192",
									"max-sheet-rows": "5000",
									"max-view-cols":"52",
									"max-view-rows":"500"
								},
								"calculationService":
								{
									"url":"http://127.0.0.1:1337/",
									"numberofretry":"3",
									"intervalofretry":"60000",
									"socketTimeout":"30000"
								}
							}
						},
						{
							"comments":"The items in this section are set by default and are not allowed to be customized.",
							"name":"pres",
							"class":"com.ibm.concord.presentation.PresentationDocumentService",
							"config":
							{
								"CLEnabled": "true",
								"limits":
								{
									"max-size": "102400",
									"max-image-size": "8192",
									"actual-max-size": "102400"
								},
								"mobile_limits":
								{
									"max-size": "51200",
									"max-image-size": "8192",
									"actual-max-size": "51200",
									"max-pages": "50"
								}
							}
						},
						{
							"comments":"The items in this section are set by default and are not allowed to be customized.",
							"name":"note",
							"class":"com.ibm.concord.writer.NoteDocumentService",
							"config":
							{
								"CLEnabled": "true",
								"limits":
								{
									"max-size": "51200",
									"max-image-size": "8192",
									"actual-max-size": "51200"
								},
								"mobile_limits":
								{
									"max-size": "51200",
									"max-image-size": "8192",
									"actual-max-size": "51200",
									"max-pages": "50",
									"max-graphics":"500"
								}
							}
						}
					],
					"mime-types" : {
						"application/msword" : "text",
						"application/vnd.ms-powerpoint" : "pres",
						"application/vnd.openxmlformats-officedocument.wordprocessingml.document" : "text",
						"application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" : "sheet",
						"application/vnd.openxmlformats-officedocument.presentationml.presentation" : "pres",
						"application/vnd.oasis.opendocument.text" : "text",
						"application/vnd.oasis.opendocument.text-template" : "text",
						"application/vnd.ms-excel" : "sheet",
						"application/vnd.oasis.opendocument.spreadsheet" : "sheet",
						"application/vnd.ms-excel.sheet.macroEnabled" : "sheet",
						"application/vnd.ms-excel.sheet.macroEnabled.12" : "sheet",
						"application/vnd.oasis.opendocument.spreadsheet-template" : "sheet",
						"application/vnd.oasis.opendocument.presentation" : "pres",
						"application/vnd.oasis.opendocument.presentation-template" : "pres",
						"application/ccd-text" : "text",
						"application/ccd-pres" : "pres",
						"text/csv" : "sheet",
						"text/plain" : "text",
						"application/vnd.openxmlformats-officedocument.wordprocessingml.template" : "text",
						"application/vnd.openxmlformats-officedocument.spreadsheetml.template" : "sheet",
						"application/vnd.openxmlformats-officedocument.presentationml.template" : "pres",
						"application/vnd.ibm-note" : "note"
					}
				}
			},
			{
				"id":"com.ibm.concord.session.SessionManager",
				"class":"com.ibm.concord.session.SessionManagerComponentImpl",
				"config":
				{
				}
			},
			{
				"id":"com.ibm.concord.platform.webmsg",
				"class":"com.ibm.concord.platform.webmsg.WebMessageComponentImpl",
				"config":
				{
					"adapter" :
					{
						"id" : "rtc4web",
						"class" : "com.ibm.concord.webmsg.rtc4web.Rtc4WebMessageAdapter",
						"config" :
						{
						}
					}
				}
			},
			{
				"id":"com.ibm.concord.platform.journal",
				"class":"com.ibm.concord.platform.journal.JournalComponentImpl",
				"config":
				{
					"adapters":
					[
						{
							"id" : "lcfiles",
							"class":"com.ibm.concord.lc3.journal.JournalLC3JMSPublisher",
							"config" :
							{
								"enabled" : "false"
							}
						}
					],
			      	"default" : "lcfiles"
				}
			},
			{
				"id":"com.ibm.docs.entitlement",
				"class":"com.ibm.docs.entitlement.EntitlementComponent",
				"config":
				{
					"defaultLevel":"NONE",
					"settings":
					{
						"FULL":
						{
							"coedit":"true",
							"assignment":"false",
							"conversion_during_upload":"true"
						},
						"SOCIAL":
						{
							"coedit":"true",
							"assignment":"false",
							"conversion_during_upload":"true"
						},
						"BASIC":
						{
							"coedit":"false",
							"assignment":"false",
							"conversion_during_upload":"false"
						},
						"CUSTOM_1":
						{
							"coedit":"true",
							"assignment":"false",
							"conversion_during_upload":"true"
						}
					}
				}
			},
			{
				"id":"com.ibm.concord.migration",
				"class":"com.ibm.concord.migration.MigrationComponent",
				"config":
				{
						"enabled": "false",
						"force_scan": "true",
						"interval": "2"
				}
			}
		]
	},
	"session" :
	{
		"interval" : "30",
		"timeout" : "30",
		"retry" : "3",
		"max-active-editing" : "1000",
		"max-users-per-session" : "20",
		"login-retry" : "false"
	},
	"auto-publish" :
	{
		"default-auto-publish" : "true",
		"max-autopublish-interval" : "3600"
	},
	"auto-cehckin" :
	{
		"default-auto-cehckin" : "false"
	},
	"s2s" :
	{
		"s2s_token" : "fallseason2011"
	},
	"cookies" :
	{
		"enforce_secure" : "false"
	},
	"docentrycache" :
	{
		"timetolive-seconds" : "60"
	},
	"beta_features" :
	{
		"beta" : ["assignment"]
	},
	"typeahead" :
	{
		"showemail" : "true",
		"is_cloud" : "false"
	},
	"viewersnapshot" :
	{
		"enabled" : "true",
		"wait_viewer_reading_time" : "10"
	},
	"socialConfig" : {
		"profile": "$profile_cfg",
		"BIZCard": "$BIZCard_cfg",
		"ST": "$ST_cfg",
		"externalprofile": {
					"url": "$external_profiles_url",
					"useInitialName":false
		}
	},
	"nativefilelock":
	{
	  "enabled" : "false"
	},
	"House_Keeping" :
	{
		"frequency" : "weekly",
		"age_threshold_of_upld_conv_per_org" : "21",
		"age_threshold_of_draft_per_org" : "90",
		"size_threshold_of_draft_per_org" : "0.7",
		"age_threshold_of_rendition_cache_per_org" : "14",
		"size_threshold_of_rendition_cache_per_org" : "0.7"
	},
	"multitenancy" :
	{
		"enablement" : "$mt",
		"tenant_pattern" : "http://{org}",
		"secure_tenant_pattern" : "https://{org}"
	},
	"picture-cache-control" :
	{
	    "enabled" : "false",
		"Max-Age" : "3600"
	},
	"http-params":
	{
	  "cache-control": "public, max-age=31536000"
	},
	"Help" :
	{
		"contents_url" : "https://help.hcltechsw.com/docs/index.html",
		"text_helpurl" : "/docshelp/onprem/onprem_help/text/document/documents_frame.html",
		"sheet_helpurl" : "/docshelp/onprem/onprem_help/text/spreadsheet/spreadsheets_frame.html",
		"pres_helpurl" : "/docshelp/onprem/onprem_help/text/presentation/presentations_frame.html",
		"infocenter_url" : "https://help.hcltechsw.com/docs/index.html"
	},
	"lotusLiveIntegration":
	{
		"lotuslive_url": "",
		"themeConfig":
			{
				"enabled": "false",
				"theme_url": "/theming/theme/css/3"
			}
	},

	"ExternalCommentService":
	{
		"enabled": "false",
		"apiConfig":
			{
				"MentionUsersUri":"/api/comment/{fileid}/getCommentUserList",
				"MentionNotificationUri":"/api/mention"
			}
	},

	"log" :
	{
		"level" : "report",
		"size" : "40"
	},
	"CustomizedFonts" :
	{
		"enabled": "false",
		"general": ["Arial/Arial, Helvetica, sans-serif", "Times New Roman/Times New Roman, Times, serif"],
		"globalized":
