	},
	"WhiteDomainList": [
		"https://app.box.com",
		"https://ibm.ent.box.com"
	],
    "build-info" :
    {
        "product_name" : "HCL Connections Docs",
        "build_version" : "$build_version",
        "build_timestamp" : "$build_timestamp",
        "build_description" : "HCL Connections Docs $build_version"
    }
